<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.admin.wellrounder.TestUserProfile">



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:padding="16dp"

        >

        <TextView
            android:id="@+id/tvUserNameUP"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="username" />

        <TextView
            android:id="@+id/tvIdUP"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="7dp"
            android:text="user ID" />

        <TextView
            android:id="@+id/tvEmailUP"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="7dp"
            android:text="Email" />

        <TextView
            android:id="@+id/tvPhoneNumberUP"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="7dp"
            android:text="phoneNumber" />

        <TextView
            android:id="@+id/tvFirstUP"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="7dp"
            android:text="First" />


            <TextView
                android:id="@+id/tvMiddleUP"
                android:text="middle"

                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/tvLastNameUP"
            android:text="lastname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />



    </LinearLayout>




</LinearLayout>
